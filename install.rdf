<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>$EXT_ID</em:id>
    <em:name>$EXT_NAME</em:name>
    <em:version>$VERSION</em:version>
    <em:description>$EXT_DESC</em:description>
    <em:creator>Jens Bannmann (jens.b@web.de)</em:creator>

    <em:contributor>Arvid Axelsson</em:contributor>
    <em:contributor>John Duncan</em:contributor>
    <em:contributor>Marco de Freitas</em:contributor>
    <em:contributor>Jonas Eckerman</em:contributor>
    <em:contributor>Máté Farkas</em:contributor>
    <em:contributor>Toni Hermoso</em:contributor>
    <em:contributor>Pawel Kata</em:contributor>
    <em:contributor>Hans Christian Saustrup</em:contributor>
    <em:contributor>Andrew Taylor</em:contributor>

    <em:homepageURL>http://cs.gettysburg.edu/~duncjo01/MessageFaces</em:homepageURL>
    <em:optionsURL>chrome://messagefaces/content/preferences.xul</em:optionsURL>
    <em:iconURL>chrome://messagefaces/content/mf-icon.png</em:iconURL>

    <!-- Front End Integration Hooks (used by Extension Manager) for pref dialog -->
    <!--
	<yuck:optionsURL>chrome://messagefaces/content/preferences.xul</yuck:optionsURL>
	<yuck:optionsType>1</yuck:optionsType>
	<yuck:iconURL>chrome://messagefaces/content/mf-icon.png</yuck:iconURL>
    -->

    <em:targetApplication>
      <Description>
        <em:id>$SM_ID</em:id>
        <em:minVersion>$SM_MIN_VERSION</em:minVersion>
        <em:maxVersion>$SM_MAX_VERSION</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:targetApplication>
      <Description>
        <em:id>$TB_ID</em:id>
        <em:minVersion>$TB_MIN_VERSION</em:minVersion>
        <em:maxVersion>$TB_MAX_VERSION</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:file>
      <Description about="urn:mozilla:extension:file:messagefaces.jar">
        <em:package>content/</em:package>
        <em:locale>locale/en-US/messagefaces/</em:locale>
        <em:locale>locale/ca-AD/messagefaces/</em:locale>
        <em:locale>locale/de-DE/messagefaces/</em:locale>
        <em:locale>locale/hu-HU/messagefaces/</em:locale>
        <em:locale>locale/pl-PL/messagefaces/</em:locale>
      </Description>
    </em:file>
  </Description>
</RDF>
